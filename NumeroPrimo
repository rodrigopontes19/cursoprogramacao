public class ExercicioNumeroPrimo {
    public static void main(String[] args){

        int numero = 10;
        int contador = 0;

            for(int i = 1; i <= numero; i++){

                if(numero % i == 0){

                    contador++;

                }

            }

                    if(contador > 2){

                        System.out.printf("%d não é primo, pois ele tem %d múltiplos", numero, contador);

                    }else{

                        System.out.printf("%d é primo, pois tem apenas %d múltiplos", numero, contador);

                    }


    }
    
}
